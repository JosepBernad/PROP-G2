<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
      fx:controller="answer.AnswerController" prefHeight="720" prefWidth="1080">

    <padding>
        <Insets bottom="10" left="25" right="25" top="25"/>
    </padding>

    <Label fx:id="surveyTitle" styleClass="form-header-title"/>
    <Label fx:id="surveyDescription"/>

    <VBox fx:id="answers" minWidth="900" minHeight="600" spacing="10"/>
</VBox>
