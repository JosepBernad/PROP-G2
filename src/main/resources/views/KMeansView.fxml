<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="center" xmlns="http://javafx.com/javafx/8.0.112" minHeight="500.0"
      prefWidth="900.0"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="analysis.KMeansController">

    <padding>
        <Insets bottom="3" left="3" right="3" top="3"/>
    </padding>

    <Label fx:id="surveyTitle" styleClass="form-header-title"/>

    <Label text="Clusters:"/>
    <JFXComboBox fx:id="numberOfClusters"/>
    <JFXButton onAction="#doKmeans" text="Do magic"/>
    <HBox alignment="center" spacing="10" fx:id="clustersPane" minWidth="400"/>
    <HBox fx:id="chart" minWidth="400"/>
</VBox>
